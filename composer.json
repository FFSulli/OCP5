{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "filp/whoops": "^2.11",
        "phpstan/phpstan": "^0.12.82",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Config\\": "config/"
        }
    },
    "require": {
        "ext-http": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "swiftmailer/swiftmailer": "^6.0",
        "twig/intl-extra": "^3.3",
        "twig/twig": "^3.3"
    }
}
